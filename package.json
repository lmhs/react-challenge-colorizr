{
  "name": "react-challenge-colorizr",
  "version": "1.0.0",
  "description": "Boilerplate for second React Challenge",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node server.js",
    "build": "npm run generatePresets && cross-env NODE_ENV=production webpack --config webpack.config.production.js --progress",
    "clean": "node utils/clean.js",
    "lint": "eslint src --ext .js --ext .jsx",
    "generatePresets": "node utils/generatePresets.js",
    "gh:deploy": "npm run clean && npm run gh:build && node utils/deploy",
    "gh:build": "cross-env NODE_ENV=production webpack --config webpack.config.pages.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/react-challenge-colorizr.git"
  },
  "keywords": [
    "react",
    "react-challenge",
    "react-router",
    "redux",
    "react-redux"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com> (http://github.com/rtivital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/react-challenge-colorizr/issues"
  },
  "homepage": "https://github.com/rtivital/react-challenge-colorizr#readme",
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "colors": "^1.1.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "delete": "^0.3.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
