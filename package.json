    "gh-pages": "^0.11.0",
    "glob": "^7.0.3",
    "html-webpack-plugin": "^2.22.0",
    "jsonfile": "^2.3.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.6.0",
    "postcss-loader": "^0.8.2",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.2.0",
    "sass-resources-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "svg-sprite-loader": "0.0.26",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
